{
  "imports": {
    "lume/": "https://deno.land/x/lume@v2.2.1/",
    "prismjs/": "npm:prismjs@1.29.0/"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A --unstable-ffi -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "compilerOptions": {
    "types": [
      "lume/types.ts"
    ]
  }
}
